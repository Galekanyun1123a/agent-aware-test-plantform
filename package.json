{
  "name": "agent-aware-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export http_proxy=http://proxy-aws-us.zhenguanyu.com:8118 && export https_proxy=http://proxy-aws-us.zhenguanyu.com:8118 && concurrently -n \"next,vite\" -c \"blue,green\" \"next dev\" \"cd ./workspace && pnpm dev\"",
    "dev:next": "export http_proxy=http://proxy-aws-us.zhenguanyu.com:8118 && export https_proxy=http://proxy-aws-us.zhenguanyu.com:8118 && next dev",
    "dev:preview": "cd ./workspace && pnpm dev",
    "dev:no-proxy": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest --no-watch",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:evals": "vitest run --config vitest.evals.config.ts",
    "test:evals:watch": "vitest --config vitest.evals.config.ts",
    "setup:workspace": "node scripts/setup-workspace.js",
    "eval": "EVAL_REAL_AI=false npx tsx evals/run.ts",
    "eval:real": "EVAL_REAL_AI=true npx tsx evals/run.ts",
    "eval:task": "EVAL_REAL_AI=false npx tsx evals/run.ts --task",
    "eval:task:real": "EVAL_REAL_AI=true npx tsx evals/run.ts --task",
    "eval:list": "npx tsx evals/run.ts --list-tasks",
    "eval:categories": "npx tsx evals/run.ts --list-categories",
    "playwright:install": "npx playwright install chromium"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.64",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "ai": "^6.0.62",
    "ai-sdk-provider-claude-code": "^3.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-resizable-panels": "^4.5.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "playwright": "^1.58.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.1.0",
    "zod": "^3.23.0"
  }
}
